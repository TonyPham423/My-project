   "source": [
    "# Xây dựng hàm tính weighted_rating\n",
    "def weighted_rating(df, m=m, C=C):\n",
    "    v = df['vote_count']\n",
    "    R = df['vote_average']\n",
    "    return (v/(v+m)*R + m/(v+m)*C)"
   ]
